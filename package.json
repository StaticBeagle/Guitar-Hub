{
  "name": "myapplication",
  "version": "1.0.0",
  "description": "Brand new Jinaga Application",
  "private": true,
  "main": "dist/server/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "webpack && tsc --project src/server && npm run build:copy",
    "build:copy": "copyfiles -f views/login.html dist/server && copyfiles -f src/server/images/* dist/server/images",
    "start": "node -r tsconfig-paths/register dist/server/server",
    "dev": "npm run build:copy && concurrently -k \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "NODE_ENV=development tsc-watch --project src/server --onSuccess \"npm start\"",
    "dev:client": "webpack --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Michael L Perry",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-react": "^1.10.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/data-grid": "^4.0.0-alpha.37",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv-webpack": "^8.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "jimp": "^0.16.1",
    "jinaga": "^4.0.1",
    "jinaga-react": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.6.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig-paths": "^4.1.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.12",
    "@types/passport": "^1.0.10",
    "@types/passport-twitter": "^1.0.37",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.19",
    "react-router-dom": "^6.3.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
